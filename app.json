{
  "expo": {
    "name": "FurmanNow",
    "slug": "FurmanNow",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#110017"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCalendarsFullAccessUsageDescription": "This app requires access to the calendar"
      },
      "bundleIdentifier": "edu.furman.now"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#110017"
      },
      "permissions": [
        "WRITE_CONTACTS",
        "READ_CONTACTS",
        "CONTACTS",
        "WRITE_CALENDAR",
        "READ_CALENDAR"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Abril_Fatface_Italic.otf",
            "./assets/fonts/BarlowSemiCondensed-Medium.ttf"
          ]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "11b311bf-c24b-46e9-91c6-72e70582f774"
      }
    }
  }
}
